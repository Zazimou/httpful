{
    "name": "nategood/httpful",
    "description": "A Readable, Chainable, REST friendly, PHP HTTP Client",
    "homepage": "https://github.com/nategood/httpful",
    "license": "MIT",
    "keywords": ["http", "curl", "rest", "restful", "api", "requests"],
    "version": "0.3.2",
    "authors": [
        {
            "name": "Nate Good",
            "email": "me@nategood.com",
            "homepage": "https://nategood.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-curl": "*",
        "ext-simplexml": "*",
        "ext-xmlwriter": "*",
        "ext-dom": "*",
        "ext-fileinfo": "*"
    },
    "autoload": {
        "psr-0": {
            "Httpful": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "ext-posix": "*"
    }
}
